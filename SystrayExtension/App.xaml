<Application
    x:Class="SystrayExtension.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SystrayExtension"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Dictionary1.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <!-- app resource dictionary that can cover style, layout and content localization -->
            <SolidColorBrush x:Key="StackPanelBorderBrushLocalApp1" Color="Red"/>
            <ThemeResource x:Key="StackPanelBorderBrushSystemApp1" ResourceKey="SystemControlBackgroundBaseMediumBrush"/>
            <!--<local:MainPageViewModel x:Key="MainPageViewModel" />-->
            <!--<Page DataContext="{StaticResource MainPageViewModel}" -->
        </ResourceDictionary>
    </Application.Resources>

</Application>
